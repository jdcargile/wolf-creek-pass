# Anthropic API key -- https://console.anthropic.com/settings/keys
ANTHROPIC_API_KEY=

# UDOT Traffic API key -- https://www.udottraffic.utah.gov/developers/doc
UDOT_API_KEY=

# Google Maps API key -- https://console.cloud.google.com/apis/credentials
# Enable: Directions API
GOOGLE_MAPS_API_KEY=

# Storage backend: "dynamo" (DynamoDB + S3) or "sqlite" (local SQLite + filesystem)
STORAGE_BACKEND=dynamo

# AWS config (for LocalStack, set AWS_ENDPOINT_URL=http://localhost:4566)
AWS_ENDPOINT_URL=http://localhost:4566
AWS_DEFAULT_REGION=us-west-2
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
